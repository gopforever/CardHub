{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "@netlify/functions",
      "node"
    ],
    "noEmit": true
  },
  "include": [
    "functions/**/*.ts"
  ]
}